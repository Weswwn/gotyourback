public class Main {

	static String checkGeneralSeats;
	static String checkRestrictedSeats;
	static String courseCodeString;
	static String courseNumberString;
	static String courseSectionString;
	static String emailAddressString;
	static String phoneNumberString;

	public static void main(String[] args) throws InterruptedException {
		// what to do here
	}

	public void setCourseCodeString(String courseCode) {
		courseCodeString = courseCode;
	}

	public void setCourseNumberString(String courseNumber) {
		courseNumberString = courseNumber;
	}

	public void setCourseSectionString(String courseSection) {
		courseSectionString = courseSection;
	}

	public void setEmailAddressString(String emailAddress) {
		emailAddressString = emailAddress;
	}

	public void setPhoneNumberString(String phoneNumber) {
		phoneNumberString = phoneNumber;
	}

	public Course userInput() {
		Course course = new Course(courseCodeString, courseNumberString, courseSectionString);
		User user = new User(course, emailAddressString, phoneNumberString);
		return course;
	}
}


